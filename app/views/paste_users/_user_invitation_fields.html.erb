  <%= f.label :invite_for_curiosity, "Send Standard Invite(just for curiosity)" %>
  <%= f.check_box :invite_for_curiosity %> 
 
  <%= f.hidden_field :_destroy %>

   <%= link_to "remove", '#', class: "remove_fields" %> 
