<p>
<%= f.hidden_field :user_id,:value => current_user.id %>
  <%= f.label :email, "Email" %>
  <%= f.text_field :email %>
  <%= f.label :circle_id %>
  <%= f.collection_select :circle_id, Circle.all, :id, :name, {prompt: "Select circle"}%>

  <%= f.label :invite_for_feedback, "Send Mirror Invite(to give me feedback)" %>
