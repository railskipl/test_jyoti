<% if !@contacts.empty? %>
  <%= form_tag complete_paste_users_path, :method => :post do %>
    <% @contacts.each do |c| %>
      <%= check_box_tag "paste_users[paste_user_emails][]", c.email %>
      <%= c.email %><br/>
    <% end %>
    <%= submit_tag "Invite"%>
  <% end %>
<% end %>
