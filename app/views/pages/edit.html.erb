<h5>Editing Page</h5>

<%= form_for @page,:html => { :multipart => true }  do |f| %>

	
    <%= f.label :title %>
    <%= f.text_field :title, :class=>"form-control" %>

 
  <div class="field">

  <%= f.label :body %>>
       <%= f.cktext_area :body %>

  </div>
   <%= f.file_field :photo %>
  
   <div class="actions">
  <%= f.submit "Update",:class=>"btn btn-primary"  %>
  </div>

<% end %></br>
<%= link_to "Back",pages_path,:class=>"btn btn-primary" %>