Welcome to user dashboard

<h6>Get your contacts list
<a href="/contacts/gmail">Gmail</a><h6>
   <% if !@contacts.nil?%>

 <% @contacts.each do |c|%>
  <ul>
    <li><%= c[:name]%> :  <%= c[:email]%> %>
  </ul> 
  <% end %>
 
 <% end %>

<%= form_for Invitation.new do |f| %>
  <p>
    <div class="col-xs-4">
    <%= f.label :recipient_email, "Your Email:" %>
    <%= f.text_field :recipient_email,:class=>"form-control" %>
    <%= f.submit 'Invite',:class=>"btn btn-primary" %></div>
  </p>
<% end %>

<h3>Import Users</h3>
<%= form_tag import_users_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Upload CSV File",:class => "button button-primary" %>
<% end %>


<%= form_for PasteUser.new do |f| %>
  <p>
    <%= f.label :email, "Copy Contact Email:" %>
    <%= f.text_area :email %>
    <%= f.submit 'Paste' %>
  </p>
<% end %>





