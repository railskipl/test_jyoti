Approve <input type="checkbox" id="checkbox1"/>
<div id="autoUpdate" class="autoUpdate">
  <% name = Circle.find(UserInvitation.find_by_email(current_user.email).circle_id).name %>
  <% sponser =  Sponser.where("name like ?", name).first %>
  <% sponser.id %>
  <% sponser.sponsercfs.each do |s|%>
  <% @cf = s.custom_factor%>
  <%= @cf %>
  <% end %>
  </div>




<script>
$(document).ready(function(){
$('#checkbox1').change(function(){
if(this.checked)
$('#autoUpdate').fadeIn('fast');
else
$('#autoUpdate').fadeOut('fast');

});
});
</script>


