<%= form_for(@sponser) do |f| %>
  <% if @sponser.errors.any? %>
    <div id="error_explanation">
      
      <% @sponser.errors.full_messages.each do |message| %>
        <p class="alert-box"><%= message %></p>
      <% end %>
    </div>
  <% end %>

      <%= f.label :name, "Circle:" %>
  <%= f.collection_select :name, Circle.all, :name, :name %></br>
  <%= f.hidden_field :user_id,:value => current_user.id %>
  <%= f.fields_for :sponsercfs do |builder| %>   
  <%= render  "sponsercf_fields", :f => builder %>
  <% end %>
 <%= link_to_add_fields "Add More Custom Factors",f,:sponsercfs %></br>


  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
