<h5>Editing Individual</h5>

<%= form_for @individual,:html => { :multipart => true }  do |f| %>

	
    <%= f.label :title %>
    <%= f.text_field :title, :class=>"form-control" %>

 
  <div class="field">

  <%= f.label :body %>>
       <%= f.cktext_area :body %>

  </div>
   <%= f.file_field :pic %>
  
   <div class="actions">
  <%= f.submit "Update",:class=>"btn btn-primary"  %>
  </div>

<% end %>
<%= link_to "Back",individuals_path,:class=>"btn btn-primary" %>
