
<%#= javascript_include_tag "jquery-1.5.2.min","jquery.validate.min","validation.js" %> 
<%= form_for Feedback.new do |f| %>
<div class="control-group">
  <label for="inputName" class="control-label">Name</label>
  <div class="controls">
    <%= f.text_field :name,:placeholder=>"Name", :id=>"inputName",:class=>"form-control", :required => true%>
  </div>
</div>
<div class="control-group">
  <label for="inputEmail" class="control-label">Email</label>
  <div class="controls">
    <%= f.text_field :email,:placeholder=>"Email", :id=>"inputEmail",:class=>"form-control", :required => true %>
  </div>
</div>

<div class="control-group">
  <label for="inputDescription" class="control-label">Feedback</label>
  <div class="controls">
    <%= f.text_area :description,:placeholder=>"Feedback", :id=>"inputDescription",:class=>"form-control11", :required => true %>
  </div>
</div>
<div class="control-group">
  <div class="controls">
    <%= f.submit "Submit",:class=>"btn"  %>
    <!-- <button class="btn" type="submit">Submit</button> -->
    
  </div>
</div>
<% end %>