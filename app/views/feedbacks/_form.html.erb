

<%= form_for Feedback.new do |f| %>
<% if @feedback.errors.any? %>
  <div id="error_explanation">
    
      <% @feedback.errors.full_messages.each do |message| %>
        <p class="alert-box"><%= message %></p>
      <% end %>
    </div>
  <% end %>

              <div class="control-group">
                <label for="inputName" class="control-label">Name</label>
                <div class="controls">
                  <%= f.text_field :name,:placeholder=>"Name", :id=>"inputName",:class=>"form-control"%>
                </div>
              </div>
              <div class="control-group">
                <label for="inputEmail" class="control-label">Email</label>
                <div class="controls">
                  <%= f.text_field :email,:placeholder=>"Email", :id=>"inputEmail",:class=>"form-control", :email => true %>
                </div>
              </div>


<div class="control-group">
  <label for="inputDescription" class="control-label">Feedback</label>
  <div class="controls" >
    <%= f.text_area :description,:placeholder => "Description", :id=>"inputDescription",:class=>"form-control11", :required => true %>
  </div>
</div>
<div class="control-group">
  <div class="controls">
    <%= f.submit "Submit",:class=>"btn btn-primary" %>
    <!-- <button class="btn" type="submit">Submit</button> -->
    
  </div>
</div>
<% end %>
