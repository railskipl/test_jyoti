<%= form_for Feedback.new do |f| %>


<div class="control-group">
  <label for="inputDescription" class="control-label">Your Feedback</label>
  <div class="controls" >
    <%= f.text_area :description,:placeholder => "Description", :id=>"inputDescription",:class=>"form-control11", :required => true, :maxlength => 2000  %>
  </div>
</div>
<div class="control-group">
  <div class="controls">
    <%= f.submit "Submit",:class=>"btn btn-primary" %>
    <!-- <button class="btn" type="submit">Submit</button> -->
    
  </div>
</div>
<% end %>

