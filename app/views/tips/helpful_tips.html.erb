 <h5>Helpful Tips</h5>
  
<table class="table table-bordered"> 
    
    
  
   <% @tips.each do |tip| %>
      <tr>
      <% if tip.user_id != current_user.id %> 

        <td><%= tip.praise rescue nil%></td>
          <td> <%= link_to 'Respond', respond_tips_tips_path  %> </td>
         <% end %>
      </tr>
    <% end %>
 
</table>
