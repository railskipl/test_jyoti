<%= form_for(@influence) do |f| %>
  <% if @influence.errors.any? %>
    <div id="error_explanation">
      
      <% @influence.errors.full_messages.each do |message| %>
        <p class="alert-box"><%= message %></p>
      <% end %>
    </div>
  <% end %>


 <%= f.hidden_field :user_id,:value => current_user.id %>
 <%= f.fields_for :power_groups do |builder| %>
  <%= render  "power_group_fields", :f => builder %>
  <% end %>
 <%= link_to_add_fields "Add More",f,:power_groups%></br>

 <!-- <%= link_to "Continue",new_tip_path,:class => "btn btn-primary"%> -->

  <!-- <div class="actions">
    <%= f.submit  :class => "btn btn-primary" %>
  </div> -->
  
  <button type="submit" >Continue</button>
<% end %>
