<h1>Listing response_tips</h1>

<table>
  <thead>
    <tr>
      <th>Tip</th>
      <th>Response for tip</th>
      <th>Tag tip</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @response_tips.each do |response_tip| %>
      <tr>
        <td><%= response_tip.tip_id %></td>
        <td><%= response_tip.response_for_tip %></td>
        <td><%= response_tip.tag_tip %></td>
        <td><%= response_tip.user_id %></td>
        <td><%= link_to 'Show', response_tip %></td>
        <td><%= link_to 'Edit', edit_response_tip_path(response_tip) %></td>
        <td><%= link_to 'Destroy', response_tip, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Response tip', new_response_tip_path %>
