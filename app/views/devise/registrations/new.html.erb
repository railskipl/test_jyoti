  <%#= javascript_include_tag "jquery-1.5.2.min","validation.js" %>
<div class="container">

<h4>Sign up</h4>

<div class="row">

  <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
      <div class="col-xs-4">
  <%= f.hidden_field :invitation_token %>
  

   <%#= devise_error_messages! %>

   <!-- <div><%= resource.errors.inspect %></div> -->


    <div><%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true,:class=>"form-control" %></div>

    <div><%= f.label :password %><br />
      <%= f.password_field :password, autocomplete: "off",:class=>"form-control" %></div>

    <div><%= f.label :password_confirmation %><br />
      <%= f.password_field :password_confirmation, autocomplete: "off",:class=>"form-control" %></div>
  <div class="control-group">
    <div class="controls"><%= f.submit "Sign up",:class=>"btn btn-primary"  %>
      <%#- if controller_name != 'sessions' %>
  <%#= link_to "Sign in", new_session_path(resource_name),:class=>"btn btn-primary" %><br />
<%# end -%></div></div>
    <%= render "devise/shared/links" %>
     <% end %>
   </div>

   <div class="col-xs-1" style="text-align:center">
<p>&nbsp;</p>
<p>&nbsp;</p>
<p>&nbsp;</p>
<p>&nbsp;</p>
</div>

   </div>
 </div>
   
